JS:

({
    fetchEmailHelper : function(component, event, helper) {
       
        var action = component.get("c.fetchEmailResultArchive");
        var requestWrap = {
        
            contactID: component.get("v.recordId")
        };
        action.setParams({
        
           	"requestWrapParam": JSON.stringify(requestWrap)
        });
     
        action.setCallback(this, function(response) {
        
            var state = response.getState();
            if (state === "SUCCESS") {
            	console.log(response.getReturnValue());
            }else{
            	console.log('@@@@@ Error '+response.getError()[0].message);
			}
        });
        $A.enqueueAction(action);
    }
})


APEX:

public class ShowEmailResultArchiveController{

    
    
    public class RequestWrap{

        @AuraEnabled
        public String contactID { get; set; }
	
	public RequestWrap(){}
    }
    
    public class RespondWrap{

        @AuraEnabled
        List<Individual_Email_Result_Archive__b> emailList { get; set; }
	
	public RespondWrap(){}
    }
    
    
    @AuraEnabled
    public static RespondWrap fetchEmailResultArchive(String requestWrapParam)  {
        //declaration of wrapper
        RequestWrap reqWrap = new RequestWrap();
        RespondWrap respWrap =  new RespondWrap();
        
        //deseriliaze.
        reqWrap = (RequestWrap) JSON.deserializeStrict(requestWrapParam, RequestWrap.class);
        system.debug('######### reqWrap  '+reqWrap.contactID);
        
        respWrap.emailList = emailList;
        return respWrap;
    }
   
}
